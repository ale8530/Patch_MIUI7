    <header android:id="@id/micloud_settings" android:title="@string/xiaomi_account">
        <intent android:targetPackage="com.xiaomi.account" android:action="android.settings.XIAOMI_ACCOUNT_SYNC_SETTINGS" />
        <extra android:name="account_type" android:value="com.xiaomi" />
    </header>
    <header android:icon="@drawable/ic_account_list" android:id="@id/account_list" android:title="@string/account_list" android:fragment="com.android.settings.accounts.MiuiAccountListSettings" />
