        <intent android:action="android.intent.action.ACTION_VIEW" />
    </header>
    <header android:icon="@drawable/ic_page_layout_settings" android:id="@id/page_layout_settings" android:title="@string/title_layout_current2">
        <intent android:targetPackage="com.android.settings" android:action="android.intent.action.MAIN" android:targetClass="com.android.settings.Settings$PageLayoutActivity" />
    </header>
    <header android:icon="@drawable/ic_display_settings" android:id="@id/display_settings" android:title="@string/display_settings" android:fragment="com.android.settings.MiuiDisplaySettings" />
    <header android:icon="@drawable/ic_sound_settings" android:id="@id/sound_settings" android:title="@string/sound_settings" android:fragment="com.android.settings.MiuiSoundSettings" />
